programa {
  funcao inicio() {
    //Declarando a temperatura em fahrenheit
    inteiro temperatura

    escreva("Digite sua temperatura: \n")
    leia (temperatura)

    escreva("Sua temperatura em Celsius é de: \n")
    fahrenheit (temperatura)
    
  }
  // cálculo de transformação de celsius para fahrenheit
  funcao fahrenheit (inteiro fahrenheit){
    escreva (5* ((fahrenheit-32)/9))
  }
}
